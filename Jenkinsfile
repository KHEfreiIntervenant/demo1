pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        bat 'echo We are in build stage'
      }
    }

    stage('Test') {
      steps {
        bat 'We are in Test stage'
      }
    }

    stage('Finish !') {
      steps {
        bat 'We are in Finish stage'
      }
    }

  }
}
